{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "experimentalDecorators": [ true ],
        "object-literal-sort-keys": [ false ],
        "ordered-imports": [ false ],
        "no-console": [ false ],
        "member-access": [ false ],
        "variable-name": [ false ],
        "no-empty-interface": [ false ],
        "max-line-length": [ false ],
        "no-empty": [ false ]
    },
    "rulesDirectory": []
}
